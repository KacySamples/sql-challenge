departments
-
dept_no varchar(10) pk 
dept_name varchar(50)

dept_emp
-
emp_no varchar(10) pk FK >- salaries.emp_no
dept_no varchar(10) FK >- departments.dept_no

dept_manager
-
dept_no varchar(10) pk FK >- dept_emp.dept_no
emp_no varchar(10) 

salaries
-
emp_no varchar(10) pk FK >- employees.emp_no
salary varchar(10)

employees
-
emp_no varchar(10) pk FK >- dept_manager.emp_no
emp_title_id varchar(10)
birth_date varchar(10)
first_name varchar(10)
last_name varchar(10)
sex varchar(10)
hire_date varchar(10)

titles
-
title_id varchar(10) pk FK >- employees.emp_title_id
title varchar(10)